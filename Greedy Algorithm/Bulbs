public class Solution {
    public int bulbs(ArrayList<Integer> A) {
        
        int ans=0;
        
        for(int i=0;i<A.size();i++){
            
            if(ans%2==0 && A.get(i)==0)
            ans++;
            
            else if(ans%2==1 && A.get(i)==1)
            ans++;
            
        }
        
        
        return ans;
    }
}
